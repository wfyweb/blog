}
#man{
	padding:90/@rem;
}
/*轮播*/
#banner{
	width:100%;
	overflow:hidden;
	position:relative;
}
#banner .banner-list{
	width:1000%;
}
#banner .banner-list li{
	float:left;
	width:770/@rem;
	height:320/@rem;
}
#banner .banner-list li a{
	display:block;
	width:100%;
	height:100%;
}
#banner .banner-list li img{
	width:100%;
	height:100%;
	vertical-align:top;
}
#banner .swiper-pagination{
	position:absolute;
	right:100/@rem;
	bottom:20/@rem;
	width:100%;
	line-height:0;
	text-align:right;
}
#banner .swiper-pagination .swiper-pagination-bullet{
	display:inline-block;
	width:12/@rem;
	height:12/@rem;
	margin-right:10/@rem;
	-webkit-border-radius:6/@rem;
	-moz-border-radius:6/@rem;
	-ms-border-radius:6/@rem;
	-o-border-radius:6/@rem;
	border-radius:6/@rem;
	background:#fff;
}
#banner .swiper-pagination .swiper-pagination-bullet-active{
	background:#FFC001;
}

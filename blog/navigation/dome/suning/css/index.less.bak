*{
   margin:0;
   padding:0;
}

@rem:77rem;
.borderline{
	position:relative;
}
.borderline::after{
	content:'';
	position:absolute;
	left:0;
	top:0;
	width: 200%;
        height:200%;
	box-sizing:border-box;
	border:1px solid #ddd;
        -webkit-transform:scale(0.5);
	-moz-transform:scale(0.5);
       
        transform:scale(0.5);
        transform-origin: 0 0;
}
body{
	padding:0;
	margin:0 auto;
	background:#f2f2f2;
}
a{
	text-decoration:none;
	font-family: Arial,Helvetica,STHeiTi,sans-serif;
	color:#333;
}
img{
	
	vertical-align:top;
}
ul,li{
	list-style:none;
}
.clearfix:after{
	content:'';
	display:block;
	clear:both;
}
/*头部*/
#header{
	width:100%;
	height:90/@rem;
	background:#fabc09;
	position:relative;
	z-index:999999;
	position:fixed;
	top:0;
	left:0;
	
}
#header .he-class{
	position:absolute;
	width:44/@rem;
	height:70/@rem;
	left:24/@rem;
	top:11/@rem;
}
